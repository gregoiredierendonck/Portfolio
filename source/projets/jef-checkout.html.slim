            img srcset="../assets/images/projets/#{data.projetjefcheckout.imagetwosmall}" alt="#{data.projetjefcheckout.imagetwoalt}" class="img"
        div class="grid--one grid--margin"
          picture class="max900355"
            source srcset="../assets/images/projets/#{data.projetjefcheckout.imagethreebig}" media="only screen and ( min-width: 900px) and (-webkit-max-device-pixel-ratio: 1.25), only screen and ( min-width: 900px) and ( max-device-pixel-ratio: 1.25)" class="img"
            source srcset="../assets/images/projets/#{data.projetjefcheckout.imagethreebig2x}" media="only screen and ( min-width: 900px) and (-webkit-min-device-pixel-ratio: 1.25), only screen and ( min-width: 900px) and ( min-device-pixel-ratio: 1.25)" class="img"
            source srcset="../assets/images/projets/#{data.projetjefcheckout.imagethreesmall2x}" media="only screen and ( min-width: 1px) and (-webkit-min-device-pixel-ratio: 2), only screen and ( min-width: 1px) and ( min-device-pixel-ratio: 2)" class="img"
            img srcset="../assets/images/projets/#{data.projetjefcheckout.imagethreesmall}" alt="#{data.projetjefcheckout.imagethreealt}" class="img"
          
      div class="grid panel--white big--padding no--border" 
