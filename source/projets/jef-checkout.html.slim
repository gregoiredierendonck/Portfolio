          source srcset="../assets/images/projets/#{data.projetjefcheckout.introimgmedium2x}" media="only screen and ( min-width: 600px) and (-webkit-min-device-pixel-ratio: 1.25), only screen and ( min-width: 600px) and ( min-device-pixel-ratio: 1.25)" class="img"
          source srcset="../assets/images/projets/#{data.projetjefcheckout.introimgsmall2x}" media="only screen and ( min-width: 1px) and (-webkit-min-device-pixel-ratio: 1.25), only screen and ( min-width: 1px) and ( min-device-pixel-ratio: 1.25)" class="img"
