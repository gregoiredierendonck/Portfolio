div class="wrapper"

  section

    h1 = data.content.competences.title
    p class="intro" = data.content.competences.intro

    - data.content.competences.sections.each_with_index do |section|
      section class="grid"

        div class="g2 twoimg"
          - section.images.each_with_index do |image|
            picture
              source srcset="#{image.retinapath}" media="(min-width: 1000px)"
              source srcset="#{image.normalpath}" media="(min-width: 800px)"
              source srcset="#{image.mobilepath}" media="(min-width: 500px)"
              img srcset="#{image.path}" alt="#{image.alt}"

        div class="g2"
          h1 class="h2" = section.title
          - section.articles.each_with_index do |article|
            article
              h2 class="h3" = article.title
              p = article.p
